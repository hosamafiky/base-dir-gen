{
  "name": "base-dir-gen",
  "displayName": "Base Feature Directory Generator",
  "description": "Generate Every Feature files",
  "icon": "images/flutter.png",
  "version": "1.0.4",
  "publisher": "HussamAbed",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "base-dir-gen.createCleanArchFolderMedina",
        "title": "BaseDirGen: New Clean Arch Feature[Medina]"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "base-dir-gen.createCleanArchFolderMedina"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hosamafiky/base-dir-gen.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.43",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1"
  }
}
